lue="0">          <!-- write documentation for File OSType (0) here-->        </ex4D:constant>      </ex4D:constant-group>      <ex4D:constant-group constant-group-id="2" name="Path Style" number="20001">        <ex4D:constant constant-id="1" name="Path Style HFS" type="L" value="0">          <!-- write documentation for Path Style HFS (0) here-->        </ex4D:constant>        <ex4D:constant constant-id="2" name="Path Style POSIX" type="L" value="1">          <!-- write documentation for Path Style POSIX (1) here-->        </ex4D:constant>        <ex4D:constant constant-id="3" name="Path Style URL" type="L" value="2">          <!-- write documentation for Path Style URL (2) here-->        </ex4D:constant>      </ex4D:constant-group>      <ex4D:constant-group constant-group-id="3" name="Launch Option" number="20002">        <ex4D:constant constant-id="1" name="Launch And Print" type="L" value="2">          <!-- write documentation for Launch And Print (2) here-->        </ex4D:constant>        <ex4D:constant constant-id="2" name="Launch Without Activation" type="L" value="512">          <!-- write documentation for Launch Without Activation (512) here-->        </ex4D:constant>        <ex4D:constant constant-id="3" name="Launch And Hide Others" type="L" value="2097152">          <!-- write documentation for Launch And Hide Others (2097152) here-->        </ex4D:constant>        <ex4D:constant constant-id="4" name="Launch And Hide" type="L" value="1048576">          <!-- write documentation for Launch And Hide (1048576) here-->        </ex4D:constant>        <ex4D:constant constant-id="5" name="Launch New Instance" type="L" value="524288">          <!-- write documentation for Launch New Instance (524288) here-->        </ex4D:constant>        <ex4D:constant constant-id="6" name="Launch Asyncronous" type="L" value="65536">          <!-- write documentation for Launch Asyncronous (65536) here-->        </ex4D:constant>        <ex4D:constant constant-id="7" name="Launch Without Adding Recents" type="L" value="256">          <!-- write documentation for Launch Without Adding Recents (256) here-->        </ex4D:constant>      </ex4D:constant-group>      <ex4D:constant-group constant-group-id="4" name="Finder Operation" number="20003">        <ex4D:constant constant-id="1" name="Operation Move" type="L" value="0">          <!-- write documentation for Operation Move (0) here-->        </ex4D:constant>        <ex4D:constant constant-id="2" name="Operation Copy" type="L" value="1">          <!-- write documentation for Operation Copy (1) here-->        </ex4D:constant>        <ex4D:constant constant-id="3" name="Operation Link" type="L" value="2">          <!-- write documentation for Operation Link (2) here-->        </ex4D:constant>        <ex4D:constant constant-id="4" name="Operation Compress" type="L" value="3">          <!-- write documentation for Operation Compress (3) here-->        </ex4D:constant>        <ex4D:constant constant-id="5" name="Operation Decompress" type="L" value="4">          <!-- write documentation for Operation Decompress (4) here-->        </ex4D:constant>        <ex4D:constant constant-id="6" name="Operation Encrypt" type="L" value="5">          <!-- write documentation for Operation Encrypt (5) here-->        </ex4D:constant>        <ex4D:constant constant-id="7" name="Operation Decrypt" type="L" value="6">          <!-- write documentation for Operation Decrypt (6) here-->        </ex4D:constant>        <ex4D:constant constant-id="8" name="Operation Destroy" type="L" value="7">          <!-- write documentation for Operation Destroy (7) here-->        </ex4D:constant>        <ex4D:constant constant-id="9" name="Operation Recycle" type="L" value="8">          <!-- write documentation for Operation Recycle (8) here-->        </ex4D:constant>        <ex4D:constant constant-id="10" name="Operation Duplicate" type="L" valu
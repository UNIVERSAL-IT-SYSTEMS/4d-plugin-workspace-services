<?xml version="1.0" encoding="UTF-8" standalone="no" ?><ex4D:plugin xmlns:ex4D="http://www.4d.com/plugin">  <!--Workspace Services-->  <ex4D:documentation name="Workspace Services" plugin-id="20000">    <ex4D:plugin-description>      <!-- write documentation for Workspace Services here -->        <p>This plugin provides useful functions from the NSFileManager and NSWorkSpace classes on Mac OS X.</p>    </ex4D:plugin-description>    <ex4D:commands>      <ex4D:command-group command-group-id="1" name="Finder">        <ex4D:command command-id="1" name="FINDER Perform operation">          <ex4D:command-description>            <!-- write documentation for FINDER Perform operation here -->          </ex4D:command-description>          <ex4D:argument argument-id="1" label="source" type="T">            <!-- write documentation for $1 (TEXT) here-->              <p>The full path of the source directory on which to perform the operation.</p>          </ex4D:argument>          <ex4D:argument argument-id="2" label="destination" type="T">            <!-- write documentation for $2 (TEXT) here-->               <p>The full path of the destination directory on which to perform the operation.</p>          </ex4D:argument>          <ex4D:argument argument-id="3" label="files" type="Y">            <!-- write documentation for $3 (ARRAY) here-->              <p>Array of file names in the source directory on which to perform the operation.</p>          </ex4D:argument>          <ex4D:argument argument-id="4" label="operation" type="L">            <!-- write documentation for $4 (LONGINT) here-->              <p>The type of operation to perform.</p>          </ex4D:argument>          <ex4D:return-value label="success" type="L">            <!-- write documentation for return value (LONGINT) here-->              <p>1 on success.</p>          </ex4D:return-value>        </ex4D:command>        <ex4D:command command-id="2" name="FINDER Get icon for file type">          <ex4D:command-description>            <!-- write documentation for FINDER Get icon for file type here -->              <p>Get the icon image for a given file type.</p>              <p>You can use the following constants as file type to get one of the standard Apple icons.</p>              <ul>                  <li>kClipboardIcon = &apos;CLIP&apos;</li>                  <li>kClippingUnknownTypeIcon = &apos;clpu&apos;</li>                  <li>kClippingPictureTypeIcon = &apos;clpp&apos;</li>                  <li>kClippingTextTypeIcon = &apos;clpt&apos;</li>                  <li>kClippingSoundTypeIcon = &apos;clps&apos;</li>                  <li>kDesktopIcon = &apos;desk&apos;</li>                  <li>kFinderIcon = &apos;FNDR&apos;</li>                  <li>kFontSuitcaseIcon = &apos;FFIL&apos;</li>                  <li>kFullTrashIcon = &apos;ftrh&apos;</li>                  <li>kGenericApplicationIcon = &apos;APPL&apos;</li>                  <li>kGenericCDROMIcon = &apos;cddr&apos;</li>                  <li>kGenericControlPanelIcon = &apos;APPC&apos;</li>                  <li>kGenericControlStripModuleIcon = &apos;sdev&apos;</li>                  <li>kGenericComponentIcon = &apos;thng&apos;</li>                  <li>kGenericDeskAccessoryIcon = &apos;APPD&apos;</li>                  <li>kGenericDocumentIcon = &apos;docu&apos;</li>                  <li>kGenericEditionFileIcon = &apos;edtf&apos;</li>                  <li>kGenericExtensionIcon = &apos;INIT&apos;</li>                  <li>kGenericFileServerIcon = &apos;srvr&apos;</li>                  <li>kGenericFontIcon = &apos;ffil&apos;</li>                  <li>kGenericFontScalerIcon = &apos;sclr&apos;</li>                  <li>kGenericFloppyIcon = &apos;flpy&apos;</li>                  <li>kGenericHardDiskIcon = &apos;hdsk&apos;</li>                  <li>kGenericIDiskIcon = &apos;idsk&apos;</li>                  <li>kGenericRemovableMediaIcon = &apos;rmov&apos;</li>                  <li>kGenericMoverObjectIcon = &apos;movr&apos;</li>                  <li>kGenericPCCardIcon = &apos;pcmc&apos;</li>                  <li>kGenericPreferencesIcon = &apos;pref&apos;</li>                  <li>kGenericQueryDocumentIcon = &apos;qery&apos;</li>                  <li>kGenericRAMDiskIcon = &apos;ramd&apos;</li>                  <li>kGenericSharedLibaryIcon = &apos;shlb&apos;</li>                  <li>kGenericStationeryIcon = &apos;sdoc&apos;</li>                  <li>kGenericSuitcaseIcon = &apos;suit&apos;</li>                  <li>kGenericURLIcon = &apos;gurl&apos;</li>                  <li>kGenericWORMIcon = &apos;worm&apos;</li>                  <li>kInternationalResourcesIcon = &apos;ifil&apos;</li>                  <li>kKeyboardLayoutIcon = &apos;kfil&apos;</li>                  <li>kSoundFileIcon = &apos;sfil&apos;</li>                  <li>kSystemSuitcaseIcon = &apos;zsys&apos;</li>                  <li>kTrashIcon = &apos;trsh&apos;</li>                  <li>kTrueTypeFontIcon = &apos;tfil&apos;</li>                  <li>kTrueTypeFlatFontIcon = &apos;sfnt&apos;</li>                  <li>kTrueTypeMultiFlatFontIcon = &apos;ttcf&apos;</li>                  <li>kUserIDiskIcon = &apos;udsk&apos;</li>                  <li>kUnknownFSObjectIcon = &apos;unfs&apos;</li>              </ul>          </ex4D:command-description>          <ex4D:argument argument-id="1" label="file" type="T">            <!-- write documentation for $1 (TEXT) here-->              <p>The type of file.</p>          </ex4D:argument>          <ex4D:argument argument-id="2" label="type" type="L">            <!-- write documentation for $2 (LONGINT) here-->              <p>The type of identification used in the previous argument.</p>          </ex4D:argument>          <ex4D:return-value label="icon" type="P">            <!-- write documentation for return value (PICTURE) here-->              <p>The icon image in TIFF format.</p>          </ex4D:return-value>        </ex4D:command>      </ex4D:command-group>      <ex4D:command-group command-group-id="2" name="Application">        <ex4D:command command-id="3" name="APPLICATION Get path">          <ex4D:command-description>            <!-- write documentation for APPLICATION Get path here -->              <p>Get the full path of a named application.</p>          </ex4D:command-description>          <ex4D:argument argument-id="1" label="application" type="T">            <!-- write documentation for $1 (TEXT) here-->              <p>The application name, with or without the .app extension. Alternatively the bundle identifier.</p>          </ex4D:argument>          <ex4D:argument argument-id="2" label="type" type="L">            <!-- write documentation for $2 (LONGINT) here-->              <p>The format in which to receive the paths.</p>          </ex4D:argument>          <ex4D:return-value label="path" type="T">            <!-- write documentation for return value (TEXT) here-->              <p>The full path of the application.</p>          </ex4D:return-value>        </ex4D:command>        <ex4D:command command-id="4" name="APPLICATION Launch">          <ex4D:command-description>            <!-- write documentation for APPLICATION Launch here -->              <p>Launch a named application.</p>          </ex4D:command-description>          <ex4D:argument argument-id="1" label="application" type="T">            <!-- write documentation for $1 (TEXT) here-->              <p>The application name, with or without the .app extension. Alternatively the bundle identifier.</p>          </ex4D:argument>          <ex4D:argument argument-id="2" label="deactivate" type="L">            <!-- write documentation for $2 (LONGINT) here-->              <p>Whether to deactivate the current application or keep active.</p>          </ex4D:argument>          <ex4D:return-value label="success" type="L">            <!-- write documentation for return value (LONGINT) here-->              <p>1, on success.</p>          </ex4D:return-value>        </ex4D:command>        <ex4D:command command-id="5" name="APPLICATION Launch paths">          <ex4D:command-description>            <!-- write documentation for APPLICATION Launch paths here -->              <p>Launch an application and perform actions on an array of documents.</p>          </ex4D:command-description>          <ex4D:argument argument-id="1" label="application" type="T">            <!-- write documentation for $1 (TEXT) here-->              <p>The application name, with or without the .app extension. Alternatively the bundle identifier.</p>          </ex4D:argument>          <ex4D:argument argument-id="2" label="action" type="L">            <!-- write documentation for $2 (LONGINT) here-->              <p>Flags to specify the kind of action to perform on the documents.</p>          </ex4D:argument>          <ex4D:argument argument-id="3" label="files" type="Y">            <!-- write documentation for $3 (ARRAY) here-->              <p>Array of files paths.</p>          </ex4D:argument>          <ex4D:return-value label="success" type="L">            <!-- write documentation for return value (LONGINT) here-->              <p>1, on succes.</p>          </ex4D:return-value>        </ex4D:command>      </ex4D:command-group>      <ex4D:command-group command-group-id="3" name="File">        <ex4D:command command-id="6" name="FILE Get localized name">          <ex4D:command-description>            <!-- write documentation for FILE Get localized name here -->              <p>Get the display name of a file system item.</p>          </ex4D:command-description>          <ex4D:argument argument-id="1" label="file" type="T">            <!-- write documentation for $1 (TEXT) here-->              <p>The full path to the file or folder.</p>          </ex4D:argument>          <ex4D:return-value label="name" type="T">            <!-- write documentation for return value (TEXT) here-->              <p>The display name, or the original path name if undefined.</p>          </ex4D:return-value>        </ex4D:command>        <ex4D:command command-id="7" name="FILE Open with application">          <ex4D:command-description>            <!-- write documentation for FILE Open with application here -->              <p>Open a file with the default application or the specified application.</p>          </ex4D:command-description>          <ex4D:argument argument-id="1" label="file" type="T">            <!-- write documentation for $1 (TEXT) here-->              <p>The full path to the document to open.</p>          </ex4D:argument>          <ex4D:argument argument-id="2" label="application" type="T">            <!-- write documentation for $2 (TEXT) here-->              <p>The application name, with or without the .app extension. Alternatively, the bundle identifier.</p>          </ex4D:argument>          <ex4D:argument argument-id="3" label="action" type="L">            <!-- write documentation for $3 (LONGINT) here-->              <p>Flags to specify the kind of action to perform on the documents.</p>          </ex4D:argument>          <ex4D:return-value label="success" type="L">            <!-- write documentation for return value (LONGINT) here-->              <p>1, on success.</p>          </ex4D:return-value>        </ex4D:command>        <ex4D:command command-id="8" name="FILE Get application name">          <ex4D:command-description>            <!-- write documentation for FILE Get application name here -->              <p>Get the name of the default application assigned to the given file.</p>          </ex4D:command-description>          <ex4D:argument argument-id="1" label="file" type="T">            <!-- write documentation for $1 (TEXT) here-->              <p>The full path to the file.</p>          </ex4D:argument>          <ex4D:return-value label="application" type="T">            <!-- write documentation for return value (TEXT) here-->              <p>The name of the application.</p>          </ex4D:return-value>        </ex4D:command>        <ex4D:command command-id="9" name="FILE SET ICON">          <ex4D:command-description>            <!-- write documentation for FILE SET ICON here -->              <p>Set the custom icon of a given file or folder.</p>          </ex4D:command-description>          <ex4D:argument argument-id="1" label="file" type="T">            <!-- write documentation for $1 (TEXT) here-->              <p>The full path to the file or folder.</p>          </ex4D:argument>          <ex4D:argument argument-id="2" label="icon" type="P">            <!-- write documentation for $2 (PICTURE) here-->              <p>The icon image.</p>          </ex4D:argument>        </ex4D:command>        <ex4D:command command-id="10" name="FILE Get icon">          <ex4D:command-description>            <!-- write documentation for FILE Get icon here -->              <p>Get the custom icon of a given file or folder.</p>          </ex4D:command-description>          <ex4D:argument argument-id="1" label="file" type="T">            <!-- write documentation for $1 (TEXT) here-->              <p>The full path to the file or folder.</p>          </ex4D:argument>          <ex4D:return-value label="icon" type="P">            <!-- write documentation for return value (PICTURE) here-->              <p>The icon image in TIFF format.</p>          </ex4D:return-value>        </ex4D:command>      </ex4D:command-group>      <ex4D:command-group command-group-id="4" name="Dock">        <ex4D:command command-id="11" name="DOCK SET BADGE LABEL">          <ex4D:command-description>            <!-- write documentation for DOCK SET BADGE LABEL here -->              <p>Set the label on the application&apos;s dock icon.</p>          </ex4D:command-description>          <ex4D:argument argument-id="1" label="label" type="T">            <!-- write documentation for $1 (TEXT) here-->              <p>The label text.</p>          </ex4D:argument>        </ex4D:command>        <ex4D:command command-id="12" name="DOCK Get icon">          <ex4D:command-description>            <!-- write documentation for DOCK Get icon here -->              <p>Get the dock icon of the application.</p>          </ex4D:command-description>          <ex4D:return-value label="icon" type="P">            <!-- write documentation for return value (PICTURE) here-->              <p>The icon image in TIFF format.</p>          </ex4D:return-value>        </ex4D:command>        <ex4D:command command-id="13" name="DOCK Get badge label">          <ex4D:command-description>            <!-- write documentation for DOCK Get badge label here -->              <p>Get the label on the application&apos;s dock icon.</p>          </ex4D:command-description>          <ex4D:return-value label="label" type="T">            <!-- write documentation for return value (TEXT) here-->              <p>The label text.</p>          </ex4D:return-value>        </ex4D:command>        <ex4D:command command-id="14" name="DOCK SET ICON">          <ex4D:command-description>            <!-- write documentation for DOCK SET ICON here -->              <p>Set the dock icon of the application.</p>          </ex4D:command-description>          <ex4D:argument argument-id="1" label="icon" type="P">            <!-- write documentation for $1 (PICTURE) here-->              <p>The icon image.</p>          </ex4D:argument>        </ex4D:command>      </ex4D:command-group>      <ex4D:command-group command-group-id="5" name="Full Screen (QuickTime)">        <ex4D:command command-id="15" name="FULL SCREEN BEGIN">          <ex4D:command-description>            <!-- write documentation for FULL SCREEN BEGIN here -->              <p>Start full screen mode using the deprecated QuickTime API. This is not the same as the OS X Lion feature.</p>          </ex4D:command-description>          <ex4D:argument argument-id="1" label="width" type="L">            <!-- write documentation for $1 (LONGINT) here-->              <p>The new screen width or 0 to keeo the current resolution.</p>          </ex4D:argument>          <ex4D:argument argument-id="2" label="height" type="L">            <!-- write documentation for $2 (LONGINT) here-->              <p>The new screen height or 0 to keeo the current resolution.</p>          </ex4D:argument>          <ex4D:argument argument-id="3" label="menu" type="L">            <!-- write documentation for $3 (LONGINT) here-->              <p>Optionally how to manage the menu.</p>          </ex4D:argument>        </ex4D:command>        <ex4D:command command-id="16" name="FULL SCREEN END">          <ex4D:command-description>            <!-- write documentation for FULL SCREEN END here -->              <p>End full screen mode. Note that the plugin will automatically end full screen mode when the database is closed.</p>          </ex4D:command-description>        </ex4D:command>      </ex4D:command-group>      <ex4D:command-group command-group-id="6" name="Folder">        <ex4D:command command-id="17" name="FOLDER GET CONTENTS">          <ex4D:command-description>            <!-- write documentation for FOLDER GET CONTENTS here -->              <p>Get paths of files in a given directory at the primary level.</p>          </ex4D:command-description>          <ex4D:argument argument-id="1" label="folder" type="T">            <!-- write documentation for $1 (TEXT) here-->              <p>The full path to the folder. May or may not have the terminator folder separator.</p>          </ex4D:argument>          <ex4D:argument argument-id="2" label="paths" type="Y">            <!-- write documentation for $2 (ARRAY) here-->              <p>Array of files paths in the specified